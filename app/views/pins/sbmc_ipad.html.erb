  <!--    container    -->
  <div id="the-wrap">
    <div class="container">
      <div class="banner-sbmc" onload="stickyScroll();">
        <!-- <a class="banner-link" href="http://www.rollouttheribbons.com/" target="_blank">&nbsp;</a> -->
      </div>
      <div id="sticky-search"></div>
      <div class="searchbar">
        <div class="searchbar-contents">
          <a class="pull-right" href="http://www.barnabashealth.org/Breast-Cancer-Awareness.aspx" target="_blank"><div class="donate"></div></a>
          <img class="search-icon" src="https://s3.amazonaws.com/myfangate.com/pinkcrusade2015/search.png" />
          <form class="searchform">
            <input class="searchfield" type="text" value="Search..." onfocus="if (this.value == 'Search...') {this.value = '';}" onblur="if (this.value == '') {this.value = 'Search...';}" />
          </form>
        </div>
        <div class="steps" >
          <div class="steps-image"></div>
        </div>
      </div>


      <div class="background" style="position:relative">
        <div class="drag-container" style="position:relative; width:100%; height:100%;">
          <div class="drag"></div>
        </div>
      </div>


      <div class="ribbonbar">

        <div class="bar">
          <div class="ribbon">
            <img src="https://s3.amazonaws.com/myfangate.com/pinkcrusade2014SBMC/img/ribbon.png" />
          </div>
          <div class="instruction">
            <img src="https://s3.amazonaws.com/myfangate.com/pinkcrusade2015/instruction.png" />
          </div>
        </div>
      </div>

      <!-- Button to trigger modal -->
      <!--
      <br/><br/><br/>
      <a href="#info" role="button" data-toggle="modal">Launch</a> <a href="#share" role="button" data-toggle="modal">Thanks</a> <a href="#" class="tooltip" title="<span style='float:right;'><a href='#'><img src='img/facebook.png' /></a>  <a href='#'><img src='img/twitter.png' /></a></span><span style='float:left;'><h2>John Doe</h2>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</span>">Tooltip</a> <a href="#" id="OpenTT">OpenTT</a>
       -->

      <!-- Modal -->
      <div id="info" class="modal shadow hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
          <h3 id="myModalLabel">Dedicate Your Ribbon</h3>
        </div>
        <div class="modal-body">
          <form class="info">
            <div>
              <div class="clearfix">
                <input type="text" name="first_name" value="First Name" placeholder="First Name" class="fullwidth padded" onfocus="if (this.value == 'First Name') {this.value = '';}" onblur="if (this.value == '') {this.value = 'First Name';}"/>
              </div>
              <div class="clearfix">
                <input type="text" name="last_name" value="Last Name" placeholder="Last Name" class="fullwidth padded" onfocus="if (this.value == 'Last Name') {this.value = '';}" onblur="if (this.value == '') {this.value = 'Last Name';}"/>
              </div>
              <div class="clearfix">
                <input type="text" name="email" value="E-Mail Address" placeholder="E-Mail Address" class="fullwidth padded" onfocus="if (this.value == 'E-Mail Address') {this.value = '';}" onblur="if (this.value == '') {this.value = 'E-Mail Address';}"/>
              </div>
              <div class="clearfix">
                <input type="text" name="zip" value="Zip Code" placeholder="Zip Code" class="fullwidth padded" onfocus="if (this.value == 'Zip Code') {this.value = '';}" onblur="if (this.value == '') {this.value = 'Zip Code';}" maxlength="5"/>
              </div>
              <div class="clearfix">
                <textarea id="comment" name="comment" placeholder="Your dedication" class="fullwidth padded" onfocus="if (this.value == 'Your dedication') {this.value = '';}" onblur="if (this.value == '') {this.value = 'Your dedication';}" maxlength="60" >Your dedication</textarea>
                <span class="input-description"><span id="comment-chars-remaining">60</span> characters remaining</span>
              </div>
            </div>
            <div class="agree">
              <input type="submit"  value="Submit Your Ribbon" />
              <input type="checkbox" id="can_email" type="agree" name="can_email" checked="checked" data-default="checked" />
              <label class="checkbox-label" for="can_email" class="">Yes, I'd like to receive additional marketing emails from The Pink Crusade</label>
            </div>

          </form>
        </div>

        <div class="modal-footer">By submitting this form you confirm that you've read and agree to our <a href="https://www.barnabashealth.org/privacy/index.htm" target="_blank">Privacy Policy</a></div>
      </div>

      <div id="share" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <h1>Thanks for Sharing</h1>
        <div><a href="#" class="social-btn" data-social="facebook" data-mine="true" ><img src="https://www.myfangate.com/ci/assets/js/custom_tab_scripts/pindrop/img/FacebookShare.png" /></a></div>
        <div><a href="#" class="social-btn" data-social="twitter"  data-mine="true" ><img src="https://www.myfangate.com/ci/assets/js/custom_tab_scripts/pindrop/img/ShareTwitter.png" /></a></div>
      </div>

    </div>
  </div>
